<% titleBuilder.add(user.username) %>
<% titleBuilder.add('Repo Editor') %>
<% include header.ejs %>
<section id="repos-editor">
	<section id="repos-selector"></section>
</section>
<script type="text/javascript" src="<%= jsPath %>vendors/requirejs/js/require.js"></script>
<script type="text/javascript" src="<%= jsPath %>require.config.js"></script>
<script type="text/javascript">
	require.config({
		baseUrl: "<%=jsPath %>"
	})
	window.accessToken = '<%= user.authToken %>'

	<% if (typeof supports !== 'undefined') { %>
	window.entities = <%- JSON.stringify(supports) %>
	<% } %>

	require(['modules/repo-selector/main'])
</script>
<% include footer.ejs %>
